void keyPressed() {
  switch (key){
    case ENTER:
    screen = 1;
    break;
   
    case 'b':
    screen = 2;
    break;
    
    case 'r':
    screen = 3;
    break;
    
    case 'h':
    screen =4;
    break;
    
    case 'z':
    screen =4;
    break;
    
     case 's':
    screen =5;
    break;
  }
}
